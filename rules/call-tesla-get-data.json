{
	"debug": false,
	"conditions": {
		"check-event-type": "'${body.cron-event}' === 'Hour'"
	},
	"_conditions": {
		"check-event-type": "'${body.cron-event}' === 'Minute'",
		"check-event-time": " /T09|T10/.test( '${body.cron-event-time}' ) "
	},
	"actions": {
		"action-tag": { 
			"type": "tag", 
			"value": "call tesla get-data"
		},
        "call-tesla-get-data-0": {
			"type": "endpoint",
			"endpoint": "http://services:8115/get-data/0",
            "method": "GET",
            "headers": {
                "cron.event.time": "${body.cron-event-time}",
				"test-header-from-action": "${timestamp}"
			}
		}
	}
}
