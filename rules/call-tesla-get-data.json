{
	"debug": false,
	"conditions": {
		"check-event-type-hour": "'${body.cron-event}' === 'Hour'",
		"enabled": true
	},
	"_conditions": {
		"check-event-type-hour": "'${body.cron-event}' === 'Hour'",
		"check-event-type-minute": "'${body.cron-event}' === 'Minute'",
		"check-event-time": " /T09|T10/.test( '${body.cron-event-time}' ) "
	},
	"actions": {
		"action-tag": { 
			"type": "tag", 
			"value": "call tesla get-data"
		},
		"call-tesla-get-data-0": {
			"type": "endpoint",
			"endpoint": "${tesla-api-url-base}/get-data/0",
			"method": "GET",
			"headers": {
				"cron-event.time": "${body.cron-event-time}",
				"test-header-from-action": "${timestamp}",
				"usr": "${tesla-usr}"
			}
		},
		"rule-filter": {
			"match": ""
		}
	}
}
