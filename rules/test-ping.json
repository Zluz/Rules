{
	"conditions": {
		"check-event-type": "'${body.cron-event}' === 'Minute'",
		"enabled": "false"
	},
	"actions": {
		"action-tag": { 
			"type": "tag", 
			"value": "event - minute ping"
		},
		"call-ping-endpoint": {
			"type": "endpoint",
			"endpoint": "${rule-proc-url-base}/ping",
			"method": "GET",
			"headers": {
				"cron.event.time": "${body.cron-event-time}"
			}
		}
	}
}
