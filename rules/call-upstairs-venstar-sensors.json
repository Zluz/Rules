{
	"conditions": {
		"check-event-type": "'${body.cron-event}' === 'Minute'",
		"enabled": "true"
	},
	"actions": {
		"action-tag": { 
			"type": "tag", 
			"value": "event - call venstar sensors"
		},
        "call-venstar-up-sensors-endpoint": {
			"type": "endpoint",
			"endpoint": "${venstar-up-url-base}/query/sensors",
            "method": "GET",
            "headers": {
                "cron.event.time": "${body.cron-event-time}",
				"test-header-from-action": "${timestamp}"
			}
		}
	}
}

