{
	"conditions": {
		"check-event-type": "'${body.cron-event}' === 'Hour'",
		"enabled": "false"
	},
	"_conditions": {
		"check-event-type": "'${body.cron-event}' === 'Minute'",
		"enabled": "false"
	},
	"actions": {
		"action-tag": { 
			"type": "tag", 
			"value": "event - tesla data",
			"_expire": 9999
		},
		"pull-tesla-data": {
			"type": "endpoint",
			"_endpoint": "8115/get-data/0",
			"endpoint": "${tesla-api-url-base}/get-data/0",
			"body": "",
			"method": "GET",
			"headers": {
				"cron-event": "${body.cron-event}",
				"cron-event.time": "${body.cron-event.time}",
				"usr": "${tesla-usr}",
				"test-header": "abc123"
			}
		}
	}
}
